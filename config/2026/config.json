  "sections": [
    {
      "name": "Prematch",
      "fields": [
        {
          "title": "Scouter Initials",
          "description": "Enter the initials of the scouter.",
          "type": "text",
          "required": true,
          "code": "scouter",
          "formResetBehavior": "preserve",
          "defaultValue": ""
        },
        {
          "title": "Match Number",
          "description": "Enter the match number.",
          "type": "number",
          "required": true,
          "code": "matchNumber",
          "formResetBehavior": "increment",
          "defaultValue": 1
        },
        {
          "title": "Robot",
          "description": "The robot you are scouting this match, based on driver station position.",
          "type": "select",
          "required": true,
          "code": "robot",
          "formResetBehavior": "preserve",
          "defaultValue": "R1",
          "choices": {
            "R1": "Red 1",
            "R2": "Red 2",
            "R3": "Red 3",
            "B1": "Blue 1",
            "B2": "Blue 2",
            "B3": "Blue 3"
          }
        },
        {
          "title": "Team Number",
          "description": "The team number of the robot you're scouting.",
          "type": "number",
          "required": true,
          "code": "teamNumber",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "min": 0,
          "max": 99999
        },
        {
          "title": "Starting Position",
          "description": "The starting position of the robot.",
          "type": "select",
          "required": true,
          "code": "Prsp",
          "formResetBehavior": "reset",
          "defaultValue": "",
          "choices": {
            "R1": "Outpost Side",
            "R2": "Middle",
            "R3": "Depot Side"
          }
        },
        {
          "title": "No Show",
          "description": "Check if the robot did not show up for the match.",
          "type": "boolean",
          "required": false,
          "code": "noShow",
          "formResetBehavior": "reset",
          "defaultValue": false
        }
      ]
    },
    {
      "name": "Autonomous",
      "fields": [
        {
          "title": "Moved?",
          "description": "Check if the robot moved during autonomous.",
          "type": "boolean",
          "required": false,
          "code": "Mved",
          "formResetBehavior": "reset",
          "defaultValue": false
        },
        {
          "title": "Entered Neutral Zone?",
          "description": "Select if the robot entered the neutral zone.",
          "type": "boolean",
          "required": false,
          "code": "enterNZ",
          "formResetBehavior": "reset",
          "defaultValue": false
        },
        {
          "title": "Scoring Time",
          "description": "Time spent actively projecting fuel",
          "type": "timer",
          "required": false,
          "code": "autoClimbTime",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "outputType": "list"
        },
        {
          "title": "Scoring Accuracy (out of 10)",
          "description": "Estimate the percentage of fuel that actually made it into the goal",
          "type": "range",
          "required": false,
          "code": "autoScor",
          "formResetBehavior": "reset",
          "defaultValue": 3,
          "min": 0,
          "max": 10,
          "step": 1
        },
        {
          "title": "Climbed Tower",
          "description": "The height the robot climbed in auto",
          "type": "select",
          "required": false,
          "code": "climbedAuto",
          "formResetBehavior": "reset",
          "defaultValue": "No",
          "choices": {
            "No": "Didn't Climb",
            "L1": "Level 1",
            "F": "Failed Climb"
          }
        },
        {
          "title": "Auto Foul",
          "type": "counter",
          "required": false,
          "code": "auf",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "min": 0,
          "step": 1
        },
        {
          "title": "Won Auto?",
          "description": "Select if the robot's alliance gained more points in auto.",
          "type": "boolean",
          "required": false,
          "code": "enterWonA",
          "formResetBehavior": "reset",
          "defaultValue": false
        }
      ]
    },
    {
      "name": "Active",
      "fields": [
        {
          "title": "Scoring Time",
          "description": "Time spent actively projecting fuel",
          "type": "timer",
          "required": false,
          "code": "scoreTimeActive",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "outputType": "list"
        },
        {
          "title": "Scoring Accuracy (out of 10)",
          "description": "Estimate the percentage of fuel that actually made it into the goal",
          "type": "range",
          "required": false,
          "code": "scoreAccuracyActive",
          "formResetBehavior": "reset",
          "defaultValue": 3,
          "min": 0,
          "max": 10,
          "step": 1
        },
        {
          "title": "Passing Time",
          "description": "Time spent actively shuffling fuel",
          "type": "timer",
          "required": false,
          "code": "passTimeActive",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "outputType": "list"
        },
        {
          "title": "Overall Role",
          "description": "What did they do most? Don't overthink this. Just what do you think they spent the most time doing?",
          "type": "select",
          "required": false,
          "code": "roleActive",
          "formResetBehavior": "reset",
          "defaultValue": "No",
          "choices": {
            "Offense": "Scored",
            "Pass": "Passed from neutral",
            "Defense": "Played defense"
          }
        },
        {
          "title": "Was Robot Defended by Other Alliance?",
          "type": "boolean",
          "required": false,
          "code": "pinnedA",
          "formResetBehavior": "reset",
          "defaultValue": false
        }
      ]
    },
    {
      "name": "Inactive",
      "fields": [
        {
          "title": "Fuel Collected From Ground",
          "description": "Collected fuel from the ground",
          "type": "boolean",
          "required": false,
          "code": "FFGI",
          "formResetBehavior": "reset",
          "defaultValue": false
        },
        {
          "title": "Fuel Collected From Player",
          "description": "Collected fuel from the human player",
          "type": "boolean",
          "required": false,
          "code": "FFHP",
          "formResetBehavior": "reset",
          "defaultValue": false
        },
        {
          "title": "Collecting and Passing Time",
          "description": "Time spent actively projecting fuel into the hub",
          "type": "timer",
          "required": false,
          "code": "passTimeInactive",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "outputType": "list"
        },
        {
          "title": "Passing Zone",
          "description": "Where did they pass from?",
          "type": "select",
          "required": false,
          "code": "passingZone",
          "formResetBehavior": "reset",
          "defaultValue": "No",
          "choices": {
            "Neutral": "Neutral",
            "OP": "Opposing Alliance",
            "Both": "Both",
            "None": "None"
          }
        },
        {
          "title": "Scored for No Reason?",
          "description": "this does not include the following two seconds after transitions",
          "type": "boolean",
          "required": false,
          "code": "stupidScoring",
          "formResetBehavior": "reset",
          "defaultValue": false
        },
        {
          "title": "Played Defense?",
          "type": "boolean",
          "required": false,
          "code": "playedDefense",
          "formResetBehavior": "reset",
          "defaultValue": false
        }
      ]
    },
    {
      "name": "Endgame",
      "fields": [
        {
          "title": "Scoring Time",
          "description": "Time spent actively projecting fuel to the hub",
          "type": "timer",
          "required": false,
          "code": "scoreTimeEndgame",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "outputType": "list"
        },
        {
          "title": "Climbed Tower",
          "type": "select",
          "required": false,
          "code": "climbed",
          "formResetBehavior": "reset",
          "defaultValue": "No",
          "choices": {
            "No": "Didn't Climb",
            "L1": "Level 1",
            "L2": "Level 2",
            "L3": "Level 3",
            "F": "Failed Climb"
          }
        },
        {
          "title": "Died?",
          "type": "boolean",
          "required": false,
          "code": "died",
          "formResetBehavior": "reset",
          "defaultValue": false
        },
        {
          "title": "Tipped/Fell Over?",
          "type": "boolean",
          "required": false,
          "code": "helpIFell",
          "formResetBehavior": "reset",
          "defaultValue": false
        }
      ]
    },
    {
      "name": "Postmatch",
      "fields": [
        {
          "title": "Navigation to Neutral",
          "description": "How did the robot get to the neutral zone?",
          "type": "select",
          "required": false,
          "code": "navTN",
          "formResetBehavior": "reset",
          "defaultValue": "",
          "choices": {
            "BAR": "Under bar",
            "BUMP": "Over bump",
            "N/A": "N/A"
          }
        },
        {
          "title": "Final Score",
          "type": "text",
          "required": false,
          "code": "finalScore",
          "formResetBehavior": "reset",
          "defaultValue": "",
          "min": 0,
          "max": 4
        },
        {
          "title": "Offense Skill",
          "type": "range",
          "required": false,
          "code": "or",
          "formResetBehavior": "reset",
          "defaultValue": 3,
          "min": 0,
          "max": 5,
          "step": 1
        },
        {
          "title": "Defensive Skill",
          "type": "range",
          "required": false,
          "code": "dr",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "min": 0,
          "max": 5,
          "step": 1
        },
        {
          "title": "Yellow/Red Card",
          "type": "select",
          "required": false,
          "code": "yc",
          "formResetBehavior": "reset",
          "defaultValue": "No Card",
          "choices": {
            "No Card": "No Card",
            "Yellow": "Yellow Card",
            "Red": "Red Card"
          }
        },
        {
          "title": "Comments",
          "type": "text",
          "required": false,
          "code": "co",
          "formResetBehavior": "reset",
          "defaultValue": "",
          "min": 0,
          "max": 200
        }
      ]
    }
  ]
}
