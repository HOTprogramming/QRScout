  "sections": [
    {
      "name": "Prematch",
      "fields": [
        {
          "title": "Scouter Initials",
          "description": "Enter the initials of the scouter.",
          "type": "text",
          "required": true,
          "code": "scouter",
          "formResetBehavior": "preserve",
          "defaultValue": ""
        },
        {
          "title": "Team Number",
          "description": "The team number of the robot you're scouting.",
          "type": "number",
          "required": true,
          "code": "teamNumber",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "min": 0,
          "max": 99999
        },
        {
          "title": "Match Number",
          "description": "Enter the match number.",
          "type": "number",
          "required": true,
          "code": "matchNumber",
          "formResetBehavior": "increment",
          "defaultValue": 1
        },
        {
          "title": "Alliance",
          "description": "Select the alliance your robot is on.",
          "type": "select",
          "required": true,
          "code": "robot",
          "formResetBehavior": "preserve",
          "defaultValue": "",
          "choices": {
            "Red": "Red",
            "Blue": "Blue"
          }
        },
        {
          "title": "Starting Position",
          "description": "The starting position of the robot.",
          "type": "select",
          "required": true,
          "code": "startingPosition",
          "formResetBehavior": "reset",
          "defaultValue": "",
          "choices": {
            "Outpost": "Outpost Side",
            "Middle": "Middle",
            "Depot": "Depot Side"
          }
        },
        {
          "title": "No Show",
          "description": "Check if the robot did not show up for the match.",
          "type": "boolean",
          "required": false,
          "code": "noShow",
          "formResetBehavior": "reset",
          "defaultValue": false
        }
      ]
    },
    {
      "name": "Autonomous",
      "fields": [
        {
          "title": "Moved?",
          "description": "Check if the robot moved during autonomous.",
          "type": "boolean",
          "required": false,
          "code": "moved",
          "formResetBehavior": "reset",
          "defaultValue": false
        },
        {
          "title": "Entered Neutral Zone?",
          "description": "Select if the robot entered the neutral zone.",
          "type": "boolean",
          "required": false,
          "code": "enteredNeutral",
          "formResetBehavior": "reset",
          "defaultValue": false
        },
        {
          "title": "Scoring Time",
          "description": "Time spent actively projecting fuel",
          "type": "timer",
          "required": false,
          "code": "autoClimbTime",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "outputType": "list"
        },
        {
          "title": "Scoring Accuracy (out of 10)",
          "description": "Estimate the percentage of fuel that actually made it into the goal",
          "type": "range",
          "required": false,
          "code": "autoScore",
          "formResetBehavior": "reset",
          "defaultValue": 3,
          "min": 0,
          "max": 10,
          "step": 1
        },
        {
          "title": "Climbed Tower",
          "description": "The height the robot climbed in auto",
          "type": "select",
          "required": false,
          "code": "climbedAuto",
          "formResetBehavior": "reset",
          "defaultValue": "No",
          "choices": {
            "No": "Didn't Climb",
            "L1": "Level 1",
            "F": "Failed Climb"
          }
        },
        {
          "title": "Auto Foul",
          "type": "counter",
          "required": false,
          "code": "autoFoul",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "min": 0,
          "step": 1
        },
        {
          "title": "Won Auto?",
          "description": "Select if the robot's alliance gained more points in auto.",
          "type": "boolean",
          "required": false,
          "code": "wonAuto",
          "formResetBehavior": "reset",
          "defaultValue": false
        }
      ]
    },
    {
      "name": "Teleop",
      "fields": [
        {
          "title": "Scoring Time",
          "description": "Time spent actively projecting fuel",
          "type": "timer",
          "required": false,
          "code": "scoreTimeActive",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "outputType": "list"
        },
        {
          "title": "Scoring Accuracy (out of 10)",
          "description": "Estimate the percentage of fuel that actually made it into the goal",
          "type": "range",
          "required": false,
          "code": "scoreAccuracyActive",
          "formResetBehavior": "reset",
          "defaultValue": 3,
          "min": 0,
          "max": 10,
          "step": 1
        },
        {
          "title": "Estimated Firing Rate",
          "description": "Estimate the rate at which fuel leaves their shooter",
          "type": "range",
          "required": false,
          "code": "firingRate",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "min": 0,
          "max": 20,
          "step": 5
        },
        {
          "title": "Fuel Collected From Ground",
          "description": "Collected fuel from the ground",
          "type": "boolean",
          "required": false,
          "code": "FFGI",
          "formResetBehavior": "reset",
          "defaultValue": false
        },
        {
          "title": "Fuel Collected From Player",
          "description": "Collected fuel from the human player",
          "type": "boolean",
          "required": false,
          "code": "FFHP",
          "formResetBehavior": "reset",
          "defaultValue": false
        }
      ]
    },
    {
      "name": "Endgame",
      "fields": [
        {
          "title": "Climbed Tower",
          "type": "select",
          "required": false,
          "code": "climbedEndgame",
          "formResetBehavior": "reset",
          "defaultValue": "No",
          "choices": {
            "No": "Didn't Climb",
            "L1": "Level 1",
            "L2": "Level 2",
            "L3": "Level 3",
            "F": "Failed Climb"
          }
        },
        {
          "title": "Was Robot Defended by Other Alliance?",
          "type": "boolean",
          "required": false,
          "code": "wasDefended",
          "formResetBehavior": "reset",
          "defaultValue": false
        },
        {
          "title": "Played Defense?",
          "type": "boolean",
          "required": false,
          "code": "playedDefense",
          "formResetBehavior": "reset",
          "defaultValue": false
        }
      ]
    },
    {
      "name": "Postmatch",
      "fields": [
        {
          "title": "Final Score",
          "type": "text",
          "required": false,
          "code": "finalScore",
          "formResetBehavior": "reset",
          "defaultValue": "",
          "min": 0,
          "max": 1000
        },
        {
          "title": "Score vs Passing",
          "description": "Neither = 0, Only Shooting = 1, Only Passing = 5",
          "type": "range",
          "required": true,
          "code": "scorePassRole",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "min": 0,
          "max": 5,
          "step": 1
        },
        {
          "title": "Navigation to Neutral",
          "description": "How did the robot get to the neutral zone?",
          "type": "select",
          "required": false,
          "code": "navToNeutral",
          "formResetBehavior": "reset",
          "defaultValue": "",
          "choices": {
            "bar": "Under bar",
            "bump": "Over bump",
            "none": "N/A"
          }
        },
        {
          "title": "Died?",
          "type": "boolean",
          "required": false,
          "code": "died",
          "formResetBehavior": "reset",
          "defaultValue": false
        },
        {
          "title": "Tipped/Fell Over?",
          "type": "boolean",
          "required": false,
          "code": "helpIFell",
          "formResetBehavior": "reset",
          "defaultValue": false
        },
        {
          "title": "Comments",
          "type": "text",
          "required": false,
          "code": "comments",
          "formResetBehavior": "reset",
          "defaultValue": "",
          "min": 0,
          "max": 200
        }
      ]
    }
  ]
}
